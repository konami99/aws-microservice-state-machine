{
  "Comment": "A description of my state machine",
  "StartAt": "Choice",
  "States": {
    "Choice": {
      "Type": "Choice",
      "Choices": [
        {
          "And": [
            {
              "Variable": "$.temp",
              "IsPresent": true
            },
            {
              "Variable": "$.humidity",
              "IsPresent": true
            },
            {
              "Variable": "$.time",
              "IsPresent": true
            },
            {
              "Variable": "$.city",
              "IsPresent": true
            }
          ],
          "Next": "DynamoDB PutItem"
        }
      ],
      "Default": "Pass"
    },
    "DynamoDB PutItem": {
      "Type": "Task",
      "Resource": "arn:aws:states:::dynamodb:putItem",
      "Parameters": {
        "TableName": "newweatherdata",
        "Item": {
          "city.$": "$.city",
          "datetime.$": "$.time",
          "temperature.$": "States.JsonToString($.temp)",
          "humidity.$": "States.JsonToString($.humidity)"
        }
      },
      "End": true
    },
    "Pass": {
      "Type": "Pass",
      "End": true
    }
  }
}