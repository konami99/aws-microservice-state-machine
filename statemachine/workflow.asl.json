{
  "Comment": "A description of my state machine",
  "StartAt": "DynamoDB PutItem",
  "States": {
    "DynamoDB PutItem": {
      "Type": "Task",
      "Resource": "arn:aws:states:::dynamodb:putItem",
      "Parameters": {
        "TableName": "weatherdata",
        "Item": {
          "timestamp": {
            "S.$": "$.time"
          },
          "temperature": {
            "N.$": "$.temp"
          },
          "humidity": {
            "N.$": "$.humidity"
          }
        }
      },
      "End": true
    }
  }
}